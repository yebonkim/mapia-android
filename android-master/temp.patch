+                        foodList.remove(tournamentIndex++);
+                        if(tournamentIndex >= (foodList.size()-1)){
+                            tournamentIndex = 1;
+                            level /= 2;
+                            levelTxt.setText(Integer.toString(level) + "강");
+                            if(level == 1){
